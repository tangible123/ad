<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/password"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toEndOf="@+id/textView2" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textInputLayout" android:layout_marginEnd="68dp"
        android:layout_marginRight="68dp"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/username"
        app:layout_constraintStart_toEndOf="@+id/textView" app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="5dp" app:layout_constraintBottom_toBottomOf="@+id/textInputLayout"
        android:layout_marginEnd="68dp" android:layout_marginRight="68dp"/>
    <Button
        android:text="Login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/button5"
        app:layout_constraintStart_toStartOf="@+id/password" android:layout_marginLeft="31dp"
        app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="31dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2" android:layout_marginBottom="156dp"/>
    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textInputLayout"
        app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginBottom="33dp"
        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="1dp"
        android:layout_marginStart="1dp" android:layout_marginEnd="1dp" android:layout_marginRight="1dp"/>
    <TextView
        android:text="账号"
        android:layout_width="71dp"
        android:layout_height="39dp" android:id="@+id/textView"
        android:paddingLeft="60px"
        android:paddingTop="25px"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toStartOf="@+id/username" android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="36dp" android:layout_marginStart="36dp" android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"/>
    <TextView
        android:text="密码"
        android:layout_width="69dp"
        android:layout_height="41dp" android:id="@+id/textView2"
        android:paddingTop="25px"
        android:paddingLeft="60px"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout"
        app:layout_constraintEnd_toStartOf="@+id/password" android:layout_marginBottom="74dp"
        app:layout_constraintBottom_toTopOf="@+id/button5" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="36dp" android:layout_marginStart="36dp" android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"/>
    <androidx.constraintlayout.widget.Group android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:id="@+id/group"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="parent"/>
    <ImageView
        android:layout_width="123dp"
        android:layout_height="0dp" app:srcCompat="@drawable/quan"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="135dp"
        android:layout_marginBottom="89dp" app:layout_constraintBottom_toTopOf="@+id/textView"/>
</androidx.constraintlayout.widget.ConstraintLayout>